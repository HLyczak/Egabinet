@model IEnumerable<Egabinet.Models.TimeSheetViewModel>

<h1>Appointments:</h1>
<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Patient)</th>
            <th>@Html.DisplayNameFor(model => model.Doctor)</th>
            <th>@Html.DisplayNameFor(model => model.Room)</th>
            <th>@Html.DisplayNameFor(model => model.Date)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (TimeSheetViewModel item in Model)
        {
            <tr>
                <td>
                    @item.Patient
                </td>
                <td>
                    @item.Doctor
                </td>
                <td>
                    @item.Room
                </td>
                <td>
                    @item.Date
                </td>

            </tr>
        }
    </tbody>
</table>